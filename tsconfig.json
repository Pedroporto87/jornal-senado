{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
  }
},
"include": [
  "src/**/*.ts",
  "src/**/*.tsx",
  ".expo/types/**/*.ts",
  "expo-env.d.ts"
],
  "exclude": ["node_modules"]
}


